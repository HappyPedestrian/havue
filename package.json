{
  "name": "vuecomponents",
  "private": true,
  "description": "awesome vue components",
  "author": "HappyPedestrian",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000 --host",
    "build": "vue-tsc && vite build",
    "update:version": "tsx scripts/gen-version.ts",
    "gen:types": "vue-tsc -p tsconfig.web.json --composite false --declaration --emitDeclarationOnly",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build:internal": "pnpm --filter \"{internal/**}\" run build",
    "build:lib": "pnpm run clean:dist && pnpm run gen:types && pnpm --filter ./packages/** run build",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "prepare": "husky",
    "publish": "bash ./scripts/publish.sh"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/types": "^0.12.2",
    "@ffmpeg/util": "^0.12.1",
    "@havue/components": "workspace:^",
    "@havue/directives": "workspace:^",
    "@havue/hooks": "workspace:^",
    "@havue/solutions": "workspace:^",
    "@havue/tools": "workspace:^",
    "@types/color": "^3.0.6",
    "@types/hammerjs": "^2.0.45",
    "@vueuse/core": "^11.0.3",
    "color": "^4.2.3",
    "element-plus": "^2.7.7",
    "hammerjs": "^2.0.8",
    "havue": "workspace:^",
    "lodash": "^4.17.21",
    "mp4box": "^0.5.2",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@eslint/js": "^9.20.0",
    "@havue/build": "workspace:^",
    "@havue/build-utils": "workspace:^",
    "@pnpm/types": "^1000.2.1",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.5.2",
    "@types/web": "^0.0.203",
    "@vitejs/plugin-vue": "^5.0.5",
    "consola": "^3.4.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "fast-glob": "^3.3.3",
    "globals": "^15.15.0",
    "gulp": "^5.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "type-fest": "^4.37.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.1",
    "unbuild": "^3.5.0",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.4",
    "vite-plugin-eslint2": "^5.0.3",
    "vue-tsc": "^2.0.24"
  }
}
